        A.ICOUPON_TYPE,
        A.ISOURCE_TYPE,
        A.ISTATUS,
        A.SADD_USER,
        A.SBRIEF_DESC,
        A.SCOUPON_CODE,
        A.SCOUPON_INSTRUCTION,
        A.SCOUPON_VALIDITY_VALUE,
        A.SINVESTMENT_ID,
        A.SMEMBER_CODE,
        A.SMEMBER_ID,
        A.SMEMBER_NAME,
        A.SMERCHANT_CODE,
        A.SMERCHANT_ID,
        A.SPAY_SERIAL_NUMBER,
        A.SREMARK,
        A.SSOURCE_AC_CODE,
        A.SSOURCE_AC_NAME,
        A.SSOURCE_CODE,
        A.SSOURCE_ID,
        A.SSOURCE_INSTRUCTION,
        A.SUPATE_USER,
        A.SUSE_LIMIT_BRAND,
        A.SUSE_LIMIT_CATEGORY,
        A.SUSE_LIMIT_COMMODITY,
        A.SUSE_LIMIT_DEVICE,
        A.SUSE_TARGET_CODE,
        A.SUSE_TARGET_ID,
        A.SUSE_TARGET_INSTRUCTION,
        A.TACTUAL_USE_DATETIME,
        A.TADD_TIME,
        A.TGET_DATETIME,
        A.TUPDATE_TIME
        FROM AC_COUPON_USER A
        LEFT JOIN SH_MERCHANT_INFO B ON A.SMERCHANT_ID = B.ID
        <include refid="Entity_Where_Clause_Like"/>
        order by
        <if test="orderStr != null and orderStr != '' ">
            ${orderStr}
        </if>
        FIELD(A.ISTATUS,2,3,4,5),A.TGET_DATETIME DESC
    </select>
    <select id="queryTotalData" resultType="java.util.HashMap" parameterType="com.cloud.cang.mgr.ac.vo.CouponUserVo">
        SELECT SUM(IFNULL(A.FCOUPON_VALUE,0)) FCOUPON_VALUE_TOTAL,
