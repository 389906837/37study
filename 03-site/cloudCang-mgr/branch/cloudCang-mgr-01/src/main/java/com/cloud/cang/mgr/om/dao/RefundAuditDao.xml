    <select id="selectPageByDomainWhere" parameterType="com.cloud.cang.mgr.om.domain.RefundAuditDomain"
            resultMap="resultEntityVo">
        SELECT A.*,SMI.SNAME MERCHANT_NAME,OOR.SDEVICE_NAME DEVICE_NAME,OOR.SDEVICE_CODE DEVICE_CODE,OOR.SDEVICE_ADDRESS
        DEVICE_ADDRESS ,OOR.IPAY_TYPE
        FROM OM_REFUND_AUDIT A
        LEFT JOIN SH_MERCHANT_INFO SMI ON A.SMERCHANT_ID =SMI.ID
        LEFT JOIN OM_ORDER_RECORD OOR ON A.SORDER_ID = OOR.ID
        <include refid="Entity_Where_ClauseLike"/>
        order by
        <if test="orderStr != null and  orderStr != ''  ">
            ${orderStr}
        </if>
        FIELD(A.IAUDIT_STATUS,10,20,30) ,A.TADD_TIME DESC

        <!--A.IAUDIT_STATUS=20 DESC ,A.IAUDIT_STATUS=30 DESC,A.TAUDIT_TIME DESC-->
    </select>

    <select id="queryTotalData" parameterType="com.cloud.cang.mgr.om.domain.RefundAuditDomain"
            resultType="java.math.BigDecimal">
        SELECT SUM(IFNULL(A.FAPPLY_REFUND_AMOUNT,0))
        FROM OM_REFUND_AUDIT A
        LEFT JOIN SH_MERCHANT_INFO SMI ON A.SMERCHANT_ID =SMI.ID
        LEFT JOIN OM_ORDER_RECORD OOR ON A.SORDER_ID = OOR.ID
        <include refid="Entity_Where_ClauseLike"/>
    </select>

    <select id="selectDownloadExcelData" parameterType="java.lang.String" resultType="java.util.HashMap">
        SELECT
        A.SMERCHANT_CODE,
        SMI.SNAME MERCHANT_NAME,
        A.SREFUND_CODE,
        A.SPAY_SERIAL_NUMBER,
        A.SORDER_CODE,
        OOR.SDEVICE_CODE,
        OOR.SDEVICE_NAME,
        OOR.SDEVICE_ADDRESS,
        SCI.SCODE COMMODITY_CODE,
        SCI.SNAME COMMODITY_NAME,
        SCI.SBRAND_NAME,
        SCI.STASTE,
        SCI.ISPEC_WEIGHT,
        SCI.SSPEC_UNIT,
        SCI.SPACKAGE_UNIT,
        ORC.FCOMMODITY_PRICE,
        ORC.FREFUND_COUNT,
        ORC.FCOMMODITY_AMOUNT,
        ORC.FREFUND_AMOUNT,
        A.SMEMBER_NAME,
        A.FTOTAL_AMOUNT,
        A.FACTUAL_PAY_AMOUNT,
        A.FAPPLY_REFUND_AMOUNT,
        A.IREFUND_STATUS,
        OOR.IPAY_TYPE,
        A.TAPPLY_TIME,
        A.IAUDIT_STATUS,
        A.SAUDIT_OPER_NAME,
        A.TAUDIT_TIME,
        SCI.SSHORT_NAME
        FROM OM_REFUND_AUDIT A
        LEFT JOIN SH_MERCHANT_INFO SMI ON A.SMERCHANT_ID =SMI.ID
        LEFT JOIN OM_ORDER_RECORD OOR ON A.SORDER_ID = OOR.ID
        LEFT JOIN OM_REFUND_COMMODITY ORC ON A.ID=ORC.SREFUND_ID
        LEFT JOIN SP_COMMODITY_INFO SCI ON ORC.SCOMMODITY_ID = SCI.ID
        WHERE A.IDEL_FLAG =0
        <if test="queryCondition != null and  queryCondition != ''  ">
            AND ${queryCondition}
        </if>
        order by
        FIELD(A.IAUDIT_STATUS,10,20,30),A.TADD_TIME DESC ,
        ORC.TADD_TIME DESC,ORC.ID
    </select>

