    <select id="selectPageByDomainWhere" parameterType="com.cloud.cang.mgr.om.domain.OrderAuditDomain"
            resultMap="resultEntityVo">
        select A.*,SMI.SNAME as MERCHANT_NAME<!--,OOR.IPAY_TYPE-->
        from OM_ORDER_AUDIT AS A
        <!-- LEFT JOIN OM_ORDER_RECORD OOR ON A.SORDER_ID =OOR.ID-->
        LEFT JOIN sh_merchant_info AS smi on A.SMERCHANT_ID=smi.ID
        <include refid="Entity_Where_Clause_Like"/>
        order by
        <if test="orderStr != null and  orderStr != ''  ">
            ${orderStr}
        </if>
        FIELD(A.ISTATUS,10,20,30),A.TADD_TIME DESC,A.TADD_TIME desc,A.ID
    </select>
    <select id="queryTotalData" parameterType="com.cloud.cang.mgr.om.domain.OrderAuditDomain"
            resultType="java.math.BigDecimal">
        SELECT SUM(IFNULL(A.FTOTAL_AMOUNT,0))
        from OM_ORDER_AUDIT AS A
        LEFT JOIN sh_merchant_info AS smi on A.SMERCHANT_ID=smi.ID
        <include refid="Entity_Where_Clause_Like"/>
    </select>
    <select id="selectByDomain" parameterType="com.cloud.cang.mgr.om.domain.OrderAuditDomain"
            resultMap="downLoadResultEntityVo">
        select A.*,SMI.SNAME as
        MERCHANT_NAME,OOAC.FCOMMODITY_PRICE,OOAC.FORDER_COUNT,OOAC.FCOMMODITY_AMOUNT,
        SCI.SNAME ,SCI.SBRAND_NAME,SCI.STASTE,SCI.ISPEC_WEIGHT,SCI.SSPEC_UNIT,SCI.SPACKAGE_UNIT
        from OM_ORDER_AUDIT AS A
        LEFT JOIN sh_merchant_info AS smi on A.SMERCHANT_ID=smi.ID
        LEFT JOIN OM_ORDER_AUDIT_COMMODITY OOAC on A.id =OOAC.SORDER_ID
        LEFT JOIN SP_COMMODITY_INFO SCI ON OOAC.SCOMMODITY_ID = SCI.ID
        <include refid="Entity_Where_Clause_Like"/>
        order by
        A.THANDLER_TIME desc,
        A.TADD_TIME desc,A.ID
    </select>
